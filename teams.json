		{ "environment": "CI", "pipelines": ["(-|\\.)CI"] }
	],
	"teams": [
		{
			"name": "IO",
			"groups": ["Elasticsearch", "Kafka"],
			"environments": [
				{ "environment": "live", "pipelines": ["Kafka\\-AWS\\-Prod"] },
