			]
		},
		{
			"name": "Moonstick",
			"groups": ["Moonstick.JS"],
			"environments": [
				{ "environment": "live", "pipelines": ["^Moonstick-Live"] }
			]
		},
		{
			"name": "HotelDistribution",
			"groups": ["Hotel-Distribution"],
			"environments": [
				{ "environment": "live-accuracy-test", "pipelines": ["^Internal.GLR-Live-Accuracy-Tests$"] }
			]
